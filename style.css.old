	float: left;
	margin: 10px 25px 10px 0;
}

img.alignright {
	display: block;
	margin: 10px 0 10px 25px;
}

img.aligncenter {
	display: block;
	clear: both;
	margin: 10px auto;
}

img.alignnone {
    display: block;
